{
  "name": "UserManagementSystem",
  "version": "1.0.0",
  "scripts": {
    "start-backend": "cd back-end && npm run start:dev ",
    "start-frontend": "cd front-end && npm run dev",
    "start": "npm-run-all --parallel start-backend start-frontend",
    "install-backend": "cd back-end && npm install",
    "install-frontend": "cd front-end && npm install",
    "install-all": "npm-run-all --parallel install-backend install-frontend",
    "setup-and-start": "npm run install-all && npm run start"
  },
  "devDependencies": {
    "npm-run-all": "^4.0.0"
  }
}
